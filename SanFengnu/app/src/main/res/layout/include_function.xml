<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="109dp"
    android:layout_height="109dp">

    <Button
        android:id="@+id/include_cicle"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:background="@drawable/circle"
        android:clickable="false"
        app:layout_constraintBottom_toBottomOf="@+id/include_trun_moto_button"
        tools:layout_editor_absoluteX="1dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/include_tdp"
        android:layout_width="53dp"
        android:layout_height="107dp"
        android:background="@drawable/tdp"
        android:clickable="false"
        app:layout_constraintStart_toStartOf="@+id/include_cicle"
        tools:layout_editor_absoluteY="2dp" />

    <Button
        android:id="@+id/include_trun_modo"
        android:layout_width="53dp"
        android:layout_height="107dp"
        android:background="@drawable/trun_modo"
        android:clickable="false"
        app:layout_constraintBottom_toBottomOf="@+id/include_cicle"
        app:layout_constraintEnd_toEndOf="@+id/include_cicle" />

</android.support.constraint.ConstraintLayout>