<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FingerActivity">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/frameLayout10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/finger_background"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            app:srcCompat="@drawable/background" />

        <Button
            android:id="@+id/finger_return"
            android:layout_width="54dp"
            android:layout_height="54dp"
            android:layout_marginLeft="52dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="33dp"
            android:background="@drawable/return_image"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/finger_user_txt"
            android:layout_width="68dp"
            android:layout_height="32dp"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="128dp"
            android:background="@drawable/user"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/finger_add_image"
            android:layout_width="130dp"
            android:layout_height="28dp"
            android:layout_marginBottom="24dp"
            android:layout_marginLeft="52dp"
            android:layout_marginStart="52dp"
            android:clickable="false"
            android:background="@drawable/add_user"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <Button
            android:id="@+id/finger_del_image"
            android:layout_width="130dp"
            android:layout_height="28dp"
            android:layout_marginBottom="24dp"
            android:layout_marginEnd="54dp"
            android:layout_marginRight="54dp"
            android:clickable="false"
            android:background="@drawable/del_user"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/finger_text_1"
            android:layout_width="134dp"
            android:layout_height="3dp"
            android:layout_marginLeft="260dp"
            android:layout_marginStart="260dp"
            android:layout_marginTop="302dp"
            android:background="#ffffff"
            android:clickable="false"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/finger_text_2"
            android:layout_width="134dp"
            android:layout_height="3dp"
            android:layout_marginLeft="318dp"
            android:layout_marginStart="318dp"
            android:layout_marginTop="302dp"
            android:background="#ffffff"
            android:clickable="false"
            app:layout_constraintStart_toStartOf="@+id/finger_text_1"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/finger_text_3"
            android:layout_width="134dp"
            android:layout_height="3dp"
            android:layout_marginLeft="318dp"
            android:layout_marginStart="318dp"
            android:layout_marginTop="302dp"
            android:background="#ffffff"
            android:clickable="false"
            app:layout_constraintStart_toStartOf="@+id/finger_text_2"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/finger_text_4"
            android:layout_width="134dp"
            android:layout_height="3dp"
            android:layout_marginLeft="260dp"
            android:layout_marginStart="260dp"
            android:layout_marginTop="130dp"
            android:background="#ffffff"
            android:clickable="false"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/finger_text_1" />

        <TextView
            android:id="@+id/finger_text_5"
            android:layout_width="134dp"
            android:layout_height="3dp"
            android:layout_marginLeft="318dp"
            android:layout_marginStart="318dp"
            android:layout_marginTop="130dp"
            android:background="#ffffff"
            android:clickable="false"
            app:layout_constraintStart_toStartOf="@+id/finger_text_4"
            app:layout_constraintTop_toTopOf="@+id/finger_text_2" />

        <TextView
            android:id="@+id/finger_text_6"
            android:layout_width="134dp"
            android:layout_height="3dp"
            android:layout_marginLeft="318dp"
            android:layout_marginStart="318dp"
            android:layout_marginTop="130dp"
            android:clickable="false"
            android:background="#ffffff"
            app:layout_constraintStart_toStartOf="@+id/finger_text_5"
            app:layout_constraintTop_toTopOf="@+id/finger_text_3" />

        <TextView
            android:id="@+id/finger_note_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="88dp"
            android:textSize="28dp"
            android:visibility="invisible"
            android:clickable="false"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/finger_text_5" />

        <Button
            android:id="@+id/finger_user_1"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            app:layout_constraintBottom_toTopOf="@+id/finger_text_1"
            app:layout_constraintStart_toStartOf="@+id/finger_text_1" />

        <Button
            android:id="@+id/finger_user_2"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            app:layout_constraintBottom_toTopOf="@+id/finger_text_2"
            app:layout_constraintStart_toStartOf="@+id/finger_text_2" />

        <Button
            android:id="@+id/finger_user_3"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            app:layout_constraintBottom_toTopOf="@+id/finger_text_3"
            app:layout_constraintStart_toStartOf="@+id/finger_text_3" />


        <Button
            android:id="@+id/finger_user_4"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            app:layout_constraintBottom_toTopOf="@+id/finger_text_4"
            app:layout_constraintStart_toStartOf="@+id/finger_text_4" />

        <Button
            android:id="@+id/finger_user_5"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            app:layout_constraintBottom_toTopOf="@+id/finger_text_5"
            app:layout_constraintStart_toStartOf="@+id/finger_text_5" />

        <Button
            android:id="@+id/finger_user_6"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            app:layout_constraintBottom_toTopOf="@+id/finger_text_6"
            app:layout_constraintStart_toStartOf="@+id/finger_text_6" />

        <Button
            android:id="@+id/finger_add_user"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/trywear"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/finger_chang_passwd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:background="@drawable/trywear"
            android:text="修改密码"
            android:textSize="30dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/finger_del_user"
            app:layout_constraintStart_toEndOf="@+id/finger_add_user" />

        <Button
            android:id="@+id/finger_del_user"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/trywear"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />
    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>